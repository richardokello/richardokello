<server class="org.jpos.q2.iso.QServer" logger="Q2" name="ISOServer">
    <attr name="port" type="java.lang.Integer">4123</attr>
     <attr name="maxSessions" type="java.lang.Integer">1000</attr>
    <channel class="org.jpos.iso.channel.NACChannel" logger="Q2"
             packager="ke.tra.com.tsync.packager.Transport_Packager" header="6000018000">
        <property name="packager-logger" value="Q2"/>
        <property name="packager-realm" value="X-Switch-debug"/>
    </channel>

    <request-listener class="ke.tra.com.tsync.RequestHandlers" logger="Q2">
        <property name="space" value="transient:default"/>
        <property name="queue" value="TXNQueue"/>
        <!--property name="queue" value="TransactionQueue"/-->
        <property name="timeout" value="10000"/>
    </request-listener>

</server>
